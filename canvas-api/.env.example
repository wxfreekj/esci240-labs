# Canvas API Configuration
# Copy this file to .env and fill in your actual values
# .env is in .gitignore and will NOT be committed to Git

# Canvas API Token
# Get from: Canvas > Account > Settings > New Access Token
CANVAS_API_TOKEN=your_canvas_api_token_here

# Canvas Domain (without https://)
# Examples: canvas.instructure.com, yourschool.instructure.com
CANVAS_DOMAIN=canvas.instructure.com

# Course ID
# Find in Canvas course URL: https://canvas.../courses/COURSE_ID
CANVAS_COURSE_ID=your_course_id_here

# Lab Assignment IDs
# Run: python list_assignments.py to find these
LAB01_ASSIGNMENT_ID=
LAB02_ASSIGNMENT_ID=
LAB03_ASSIGNMENT_ID=
LAB04_ASSIGNMENT_ID=
LAB05_ASSIGNMENT_ID=
LAB06_ASSIGNMENT_ID=
LAB07_ASSIGNMENT_ID=
LAB08_ASSIGNMENT_ID=
LAB09_ASSIGNMENT_ID=
LAB10_ASSIGNMENT_ID=

# Output directories (relative to canvas-api folder)
OUTPUT_BASE_DIR=./submissions
ASSIGNMENTS_OUTPUT_FILE=./output/assignments_list.json
